  "engines": {
    "vscode": "^1.45.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "Coldark - Cold",
        "uiTheme": "vs",
        "path": "./themes/coldark-light-color-theme.json"
      },
      {
        "label": "Coldark - Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/coldark-dark-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "auto-changelog": "^2.1.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.0.0",
    "ovsx": "0.1.0-next.e000fdb",
    "vsce": "^1.81.0"
  },
  "scripts": {
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "package": "vsce package",
    "publish": "dotenv -- cross-var \"vsce publish && ovsx publish -p %OVSX_TOKEN%\""
  },
  "auto-changelog": {
    "template": "keepachangelog",
    "remote": "github",
    "commitLimit": false,
    "releaseSummary": true,
    "startingVersion": "v1.2.1"
  }
