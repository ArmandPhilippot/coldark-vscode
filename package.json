{
  "name": "coldark",
  "displayName": "Coldark",
  "description": "A blue-grey theme with light & dark versions.",
  "version": "1.0.0",
  "publisher": "ArmandPhilippot",
  "homepage": "https://github.com/ArmandPhilippot/coldark",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArmandPhilippot/coldark"
  },
  "bugs": {
    "url": "https://github.com/ArmandPhilippot/coldark/issues"
  },
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Coldark - Cold",
        "uiTheme": "vs",
        "path": "./themes/coldark-light-color-theme.json"
      },
      {
        "label": "Coldark - Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/coldark-dark-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "auto-changelog": "^2.1.0",
    "vsce": "^1.77.0"
  },
  "scripts": {
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "package": "vsce package",
    "publish": "vsce publish"
  }
}
