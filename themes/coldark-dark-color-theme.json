  "tokenColors": [
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#AFDE6E"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A5AEBA"
      }
    },
    {
      "name": "Comments Shebang",
      "scope": ["comment.line.number-sign.shebang"],
      "settings": {
        "foreground": "#6BCAF6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers & Attributes",
      "scope": ["constant.numeric", "entity.other.attribute-name"],
      "settings": {
        "foreground": "#F0D884"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color", "support.constant.color"],
      "settings": {
        "foreground": "#8FD4A6"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#6BCAF6"
      }
    },
    {
      "name": "Tag start/end",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#AFE0FF"
      }
    },
    {
      "name": "Properties",
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#6BCAF6"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#CEA0F3"
      }
    },
    {
      "name": "Functions parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#F3C2A3"
      }
    },
    {
      "name": "Variables Language",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#6BCAF6",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Keywords / Storage",
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#E4A35A"
      }
    },
    {
      "name": "PHPDoc Keywords / Function Arrow",
      "scope": ["keyword.other.phpdoc", "storage.type.function.arrow"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Support - Constants",
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#F4C19F"
      }
    },
    {
      "name": "Support - Functions",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#FF8081"
      }
    },
    {
      "name": "Support Variables",
      "scope": ["support.variable.property"],
      "settings": {
        "foreground": "#A1DADD"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#6BCAF6"
      }
    },
    {
      "name": "CSS IDs",
      "scope": ["entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#E897CB"
      }
    },
    {
      "name": "CSS classes",
      "scope": ["entity.other.attribute-name.class.css"],
      "settings": {
        "foreground": "#A5A3D5"
      }
    },
    {
      "name": "CSS Pseudo Classes & Elements",
      "scope": [
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#CEA0F3"
      }
    },
    {
      "name": "CSS - Browser prefix",
      "scope": [
        "source.css support.type.vendored",
        "source.sass support.type.vendored",
        "source.scss support.type.vendored",
        "source.less support.type.vendored",
        "source.stylus support.type.vendored"
      ],
      "settings": {
        "foreground": "#96BDCE"
      }
    },
    {
      "name": "Markup Headings",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#6BCAF6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#F0D884"
      }
    },
    {
      "name": "Markdown Punctuation definition",
      "scope": [
        "punctuation.definition.markdown",
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#CEA0F3",
        "fontStyle": "bold"
      }
    }
  ]
}
