  "tokenColors": [
    {
      "name": "Bold",
      "scope": ["strong", "markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": ["emphasis", "markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#304259",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constant Character",
      "scope": "constant.character",
      "settings": {
        "foreground": "#b800b8"
      }
    },
    {
      "name": "Constant Character Escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#b800b8"
      }
    },
    {
      "name": "Constant Language",
      "scope": "constant.language",
      "settings": {
        "foreground": "#7d6600"
      }
    },
    {
      "name": "Numeric",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#7d6600"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#005c99"
      }
    },
    {
      "name": "Functions",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#8600c6"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#007474"
      }
    },
    {
      "name": "Attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#7d6600"
      }
    },
    {
      "name": "Entity Other Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#005c99",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid, deprecated & illegal",
      "scope": "invalid",
      "settings": {
        "foreground": "#bf0100"
      }
    },
    {
      "name": "Keywords / Storage",
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#aa4d00"
      }
    },
    {
      "scope": "meta.tag.template.block",
      "settings": {
        "foreground": "#bf0100"
      }
    },
    {
      "name": "Punctuation Definition Tag",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#007474"
      }
    },
    {
      "name": "Punctuation Definition Comment",
      "scope": [
        "punctuation.definition.comment",
        "punctuation.end.definition.comment",
        "punctuation.start.definition.comment"
      ],
      "settings": {
        "foreground": "#304259"
      }
    },
    {
      "name": "Punctuation Section Embedded",
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end"
