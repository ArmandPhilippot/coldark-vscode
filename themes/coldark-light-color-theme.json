  "tokenColors": [
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#0A5800"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#576373"
      }
    },
    {
      "name": "Comments Shebang",
      "scope": ["comment.line.number-sign.shebang"],
      "settings": {
        "foreground": "#005271",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#AE7B00"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#AE7B00"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color", "support.constant.color"],
      "settings": {
        "foreground": "#255234"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#005271"
      }
    },
    {
      "name": "Tag start/end",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#4E6574"
      }
    },
    {
      "name": "Properties",
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#005271"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#6917AB"
      }
    },
    {
      "name": "Functions parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#5D4536"
      }
    },
    {
      "name": "Variables Language",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#005271",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Keywords / Storage",
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#C6633B"
      }
    },
    {
      "name": "Support - Constants",
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#9F533A"
      }
    },
    {
      "name": "Support - Functions",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#9A0100"
      }
    },
    {
      "name": "Support Variables",
      "scope": ["support.variable.property"],
      "settings": {
        "foreground": "#394F50"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#005271"
      }
    },
    {
      "name": "CSS IDs",
      "scope": ["entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#AA1174",
        "fontStyle": ""
      }
    },
    {
      "name": "CSS classes",
      "scope": ["entity.other.attribute-name.class.css"],
      "settings": {
        "foreground": "#3834B6"
      }
    },
    {
      "name": "CSS Pseudo Classes & Elements",
      "scope": [
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "foreground": "#6917AB",
        "fontStyle": ""
      }
    },
    {
      "name": "CSS - Browser prefix",
      "scope": [
        "source.css support.type.vendored",
        "source.sass support.type.vendored",
        "source.scss support.type.vendored",
        "source.less support.type.vendored",
        "source.stylus support.type.vendored"
      ],
      "settings": {
        "foreground": "#3F6E82"
      }
    },
    {
      "name": "Markup Headings",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#005271",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#AE7B00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Punctuation definition",
      "scope": [
        "punctuation.definition.markdown",
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#6917AB",
        "fontStyle": "bold"
      }
    }
  ]
}
